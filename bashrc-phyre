# ~/.bashrc on phyre

# If not running interactively, don't do anything
[[ $- != *i* ]] && return

source ~/.dotfiles/bashrc

# The next line updates PATH for the Google Cloud SDK.
if [ -f '/home/joram/bin/blob/google-cloud-sdk/path.bash.inc' ]; then source '/home/joram/bin/blob/google-cloud-sdk/path.bash.inc'; fi

# The next line enables shell command completion for gcloud.
if [ -f '/home/joram/bin/blob/google-cloud-sdk/completion.bash.inc' ]; then source '/home/joram/bin/blob/google-cloud-sdk/completion.bash.inc'; fi

alias cadar-sql-proxy='cloud_sql_proxy -instances=schakel-cloud:europe-west4:cadar-1=tcp:3317'
alias cadar-sql-readonly='mysql --host=127.0.0.1 --port=3317 --user=readonly --password cadar_db'
alias cadar-sql-dangerous='mysql --host=127.0.0.1 --port=3317 --user=cadar_usr --password cadar_db'
